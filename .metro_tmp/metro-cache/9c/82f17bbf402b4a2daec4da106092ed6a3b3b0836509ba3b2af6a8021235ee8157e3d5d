ert.default.alert(\"Notification\", \"Nouvelle demande d'ami reçue !\");\n        };\n        const handleReceiveMessage = message => {\n          if (message && message._id && message.sender) {\n            socket.default.emit('message_delivered', {\n              messageId: message._id,\n              senderId: message.sender\n            });\n\n            // Optimistic Update\n            setChats(prevChats => {\n              const chatIndex = prevChats.findIndex(c => c.friendId === message.sender);\n\n              // If chat exists, update it\n              if (chatIndex !== -1) {\n                const updatedChats = [...prevChats];\n                const chat = updatedChats[chatIndex];\n\n                // Remove from current position\n                updatedChats.splice(chatIndex, 1);\n\n                // Add to top with updated info\n                updatedChats.unshift({\n                  ...chat,\n                  lastMessage: message.content,\n                  timestamp: formatHeureRecente(new Date().toISOString()),\n                  unread: (chat.unread || 0) + 1,\n                  lastRead: '' // New message is not read by me yet (obviously)\n                });\n                return updatedChats;\n              }\n\n              // If chat doesn't exist (new friend), we need to fetch details\n              fetchData();\n              return prevChats;\n            });\n          } else {\n            fetchData();\n          }\n        };\n        const handleMessagesRead = ({\n          readerId\n        }) => {\n          // Optimistic Update: Mark last message as read if I am the sender\n          setChats(prevChats => {\n            return prevChats.map(chat => {\n              if (chat.friendId === readerId) {\n                return {\n                  ...chat,\n                  lastRead: 'Lu'\n                };\n              }\n              return chat;\n            });\n          });\n          // We can still fetch to be sure, or skip it\n          // fetchData(); \n        };\n        const handleStatusUpdate = ({\n          userId,\n          isOnline\n        }) => {\n          // Optimistic update\n          setFriends(prevFriends => prevFriends.map(f => {\n            if (f.id === userId) {\n              return {\n                ...f,\n                isOnline: isOnline,\n                lastSeen: isOnline ? null : new Date().toISOString() // If offline, set lastSeen to now\n              };\n            }\n            return f;\n          }));\n        };\n        const handleGameInvitation = data => {\n          setIncomingInvite(data);\n        };\n        const handleInvitationError = msg => {\n          setIsWaitingForResponse(false);\n          Alert.default.alert(\"Erreur\", msg);\n        };\n        const handleInvitationDeclined = data => {\n          setIsWaitingForResponse(false);\n          Alert.default.alert(\"Refusé\", `${data.recipientPseudo || 'L\\'adversaire'} a refusé l'invitation.`);\n        };\n        const handleGameStart = data => {\n          setIncomingInvite(null);\n          setIsWaitingForResponse(false);\n          navigation.navigate('Game', {\n            mode: 'online',\n            gameId: data.gameId,\n            players: data.players,\n            currentTurn: data.currentTurn,\n            betAmount: data.betAmount,\n            timeControl: data.timeControl,\n            gameType: data.mode,\n            tournamentSettings: data.tournamentSettings,\n            opponent: data.players.black.id.toString() === (user._id || user.id).toString() ? data.players.white : data.players.black\n          });\n        };\n        socket.default.on('friend_request_received', handleFriendRequest);\n        socket.default.on('receive_message', handleReceiveMessage);\n        socket.default.on('messages_read', handleMessagesRead);\n        socket.default.on('friend_status_updated', handleStatusUpdate);\n        socket.default.on('game_invitation', handleGameInvitation);\n        socket.default.on('invitation_error', handleInvitationError);\n        socket.default.on('invitation_declined', handleInvitationDeclined);\n        socket.default.on('game_start', handleGameStart);\n        socket.default.on('connect', handleConnect);\n        return () => {\n          socket.default.off('friend_request_received', handleFriendRequest);\n          socket.default.off('receive_message', handleReceiveMessage);\n          socket.default.off('messages_read', handleMessagesRead);\n          socket.default.off('friend_status_updated', handleStatusUpdate);\n          socket.default.off('game_invitation', handleGameInvitation);\n          socket.default.off('invitation_error', handleInvitationError);\n          socket.default.off('invitation_declined', handleInvitationDeclined);\n          socket.default.off('game_start', handleGameStart);\n          socket.default.off('connect', handleConnect);\n        };\n      }\n    }, [user, token, fetchData]);\n\n    // --- ACTIONS ---\n\n    const handleJoinSpectator = friend => {\n      if (!friend.currentGame) return;\n      navigation.navigate('Game', {\n        mode: 'spectator',\n        gameId: friend.currentGame\n      });\n    };\n    const handleBlockFriend = friend => {\n      // Implement block logic if API supports it\n      Alert.default.alert(\"Info\", \"Fonctionnalité de blocage à venir.\");\n    };\n    const handleRemoveFriend = friend => {\n      Alert.default.alert(\"Supprimer l'ami\", `Êtes-vous sûr de vouloir supprimer ${friend.name} de vos amis ?`, [{\n        text: \"Annuler\",\n        style: \"cancel\"\n      }, {\n        text: \"Supprimer\",\n        style: \"destructive\",\n        onPress: async () => {\n          try {\n            const res = await fetch(`${_config.API_URL}/friends/${friend.id}`, {\n              method: 'DELETE',\n              headers: {\n                Authorization: `Bearer ${token}`\n              }\n            });\n            if (res.ok) {\n              setFriends(prev => prev.filter(f => f.id !== friend.id));\n              Alert.default.alert(\"Succès\", \"Ami supprimé.\");\n            } else {\n              const data = await res.json();\n              Alert.default.alert(\"Erreur\", data.message);\n            }\n          } catch (error) {\n            Alert.default.alert(\"Erreur\", \"Erreur réseau.\");\n          }\n        }\n      }]);\n    };\n    const handleAcceptRequest = async request => {\n      try {\n        const res = await fetch(`${_config.API_URL}/friends/accept`, {\n          method: 'POST',\n          headers: {\n            'Content-Type': 'application/json',\n            Authorization: `Bearer ${token}`\n          },\n          body: JSON.stringify({\n            requestId: request.id\n          })\n        });\n        if (res.ok) {\n          // Optimistic update or refetch\n          fetchData();\n          Alert.default.alert(\"Succès\", \"Demande acceptée.\");\n        } else {\n          const data = await res.json();\n          Alert.default.alert(\"Erreur\", data.message);\n        }\n      } catch (error) {\n        Alert.default.alert(\"Erreur\", \"Erreur réseau.\");\n      }\n    };\n    const handleDeclineRequest = async id => {\n      try {\n        const res = await fetch(`${_config.API_URL}/friends/decline`, {\n          method: 'POST',\n          headers: {\n            'Content-Type': 'application/json',\n            Authorization: `Bearer ${token}`\n          },\n          body: JSON.stringify({\n            requestId: id\n          })\n        });\n        if (res.ok) {\n          setRequestsReceived(prev => prev.filter(r => r.id !== id));\n        } else {\n          const data = await res.json();\n          Alert.default.alert(\"Erreur\", data.message);\n        }\n      } catch (error) {\n        Alert.default.alert(\"Erreur\", \"Erreur réseau.\");\n      }\n    };\n    const handleCancelRequest = async id => {\n      Alert.default.alert(\"Info\", \"Annulation spécifique non implémentée.\");\n    };\n    const handleCancelLatestRequest = async () => {\n      try {\n        const res = await fetch(`${_config.API_URL}/friends/cancel-latest`, {\n          method: 'POST',\n          headers: {\n            Authorization: `Bearer ${token}`\n          }\n        });\n        const data = await res.json();\n        if (res.ok) {\n          fetchData();\n          Alert.default.alert(\"Succès\", data.message || \"Dernière demande annulée.\");\n        } else {\n          Alert.default.alert(\"Erreur\", data.message || \"Impossible d'annuler la demande.\");\n        }\n      } catch (error) {\n        Alert.default.alert(\"Erreur\", \"Erreur réseau.\");\n      }\n    };\n    const handleSendRequest = async () => {\n      if (newFriendName.trim().length === 0) return;\n      try {\n        // 1. Search user\n        const searchRes = await fetch(`${_config.API_URL}/friends/search?q=${newFriendName}`, {\n          headers: {\n            Authorization: `Bearer ${token}`\n          }\n        });\n        const responseData = await searchRes.json();\n        if (!searchRes.ok) {\n          Alert.default.alert(\"Erreur\", responseData.message || \"Erreur lors de la recherche.\");\n          return;\n        }\n        const users = responseData;\n        if (!Array.isArray(users)) {\n          Alert.default.alert(\"Erreur\", \"Format de réponse invalide.\");\n          return;\n        }\n        if (users.length === 0) {\n          Alert.default.alert(\"Erreur\", \"Utilisateur non trouvé.\");\n          return;\n        }\n\n        // 2. Select first match (simplified)\n        const targetUser = users[0]; // You might want to show a list to pick from\n\n        // 3. Send Request\n        const res = await fetch(`${_config.API_URL}/friends/request`, {\n          method: 'POST',\n          headers: {\n            'Content-Type': 'application/json',\n            Authorization: `Bearer ${token}`\n          },\n          body: JSON.stringify({\n            recipientId: targetUser._id\n          })\n        });\n        const data = await res.json();\n        if (res.ok) {\n          Alert.default.alert(\"Succès\", `Demande envoyée à ${targetUser.pseudo}`);\n          setNewFriendName('');\n          setIsAddFriendVisible(false);\n          fetchData();\n        } else {\n          Alert.default.alert(\"Erreur\", data.message);\n        }\n      } catch (error) {\n        console.error(error);\n        Alert.default.alert(\"Erreur\", \"Erreur réseau.\");\n      }\n    };\n    const handleSendInvite = () => {\n      if (!selectedFriend) return;\n      socket.default.emit('invite_friend', {\n        recipientId: selectedFriend.id,\n        betAmount: inviteBet,\n        timeControl: inviteTime,\n        mode: inviteMode,\n        seriesLength: inviteMode === 'tournament' ? inviteSeriesLength : 1\n      });\n      setInviteConfigVisible(false);\n      setIsWaitingForResponse(true);\n    };\n    const handleAcceptInvite = () => {\n      if (!incomingInvite) return;\n      socket.default.emit('respond_invite', {\n        senderId: incomingInvite.senderId,\n        accepted: true,\n        betAmount: incomingInvite.betAmount,\n        timeControl: incomingInvite.timeControl,\n        gameId: incomingInvite.gameId,\n        mode: incomingInvite.mode,\n        seriesLength: incomingInvite.seriesLength\n      });\n      setIncomingInvite(null);\n    };\n    const handleDeclineInvite = () => {\n      if (!incomingInvite) return;\n      socket.default.emit('respond_invite', {\n        senderId: incomingInvite.senderId,\n        accepted: false\n      });\n      setIncomingInvite(null);\n    };\n\n    // --- FILTERING ---\n    const filteredFriends = friends.filter(f => f.name.toLowerCase().includes(searchQuery.toLowerCase()));\n    const filteredChats = chats.filter(c => c.name.toLowerCase().includes(searchQuery.toLowerCase()));\n    const filteredReceived = requestsReceived.filter(r => r.name.toLow